<?xml version='1.0' encoding='utf-8'?>
<widget id="com.luengen.alexander" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="BLE">
        <param name="android-package" value="com.megster.cordova.ble.central.BLECentralPlugin" />
    </feature>
    <feature name="Vibration">
        <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
    </feature>
    <name>Morse Code Game</name>
    <description>
        A new haptic game, to learn the Morse code with ease.
    </description>
    <author email="alexander.luengen@student.kit.edu" href="Luengen">
        LA
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="permissions" value="none" />
</widget>
